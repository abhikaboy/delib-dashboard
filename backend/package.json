{
  "name": "delibs-backend",
  "version": "1.0.0",
  "description": "Lightweight backend for delibs fraternity application system",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "bun --watch server.js",
    "build": "echo 'No build step required'",
    "create-aggregate": "node create-aggregate.js",
    "populate-eval-data": "node populate-eval-data.js",
    "create-indexes": "node create-indexes.js",
    "compute-averages": "node compute-global-averages.js",
    "setup-all": "node create-indexes.js && node create-aggregate.js && node populate-eval-data.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "keywords": ["express", "mongodb", "api"],
  "author": "delibs",
  "license": "MIT"
}
